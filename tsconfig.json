{
    "compilerOptions": {
      "allowJs": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "removeComments": true,
      "sourceMap": true,
      "types": ["node"],
      "strict": true,
      "target": "esnext",
      "baseUrl": ".",
      "outDir": "bin",
      "paths": {
        "src/*": ["src/*"],
      }
    },
    "exclude": ["bin", "node_modules", "front"]
  }
  